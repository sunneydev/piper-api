{
  "name": "piper-api",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "node dist/app.mjs",
    "prestart": "npm run build",
    "build": "tsup src/app.ts --format esm --clean --sourcemap --minify --metafile",
    "dev": "tsup src/app.ts --format esm --watch --clean --onSuccess \"node dist/app.mjs\"",
    "caddy": "caddy reverse-proxy --from home.sunney.dev --to localhost:5000"
  },
  "keywords": [
    "piper"
  ],
  "author": "sunneydev",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.27.2",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "socket.io": "^4.2.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/node": "^18.6.2",
    "nodemon": "^2.0.19",
    "tsup": "^6.2.2",
    "typescript": "4.4.2"
  }
}
